function popupChat() {
	$('.alert-chat').show();
    $('.alert-chat').html("Carregando...");
    $(".alert-chat").addClass("alert-default");

        var nome = $('#popup_chat_nome');
        var email = $('#popup_chat_email');
        var telefone = $('#popup_chat_telefone');

        if (nome.val() == '') {
            nome.css('border', '1px solid red');
            nome.focus();
            $(".alert-chat").html('Favor preencher o campo Nome!');
            $(".alert-chat").addClass("alert-danger");

            return false;
        } else {
            nome.css('border', '1px solid #818181');
        }
        if (email.val() == '') {
            email.css('border', '1px solid red');
            email.focus();
            $(".alert-chat").html('Favor preencher o campo E-mail!');
            $(".alert-chat").addClass("alert-danger");
            return false;
        } else {
            email.css('border', '1px solid #818181');
        }
        if (!validateEmail(email.val())) {
            email.css('border', '1px solid red');
            email.focus();
            $(".alert-chat").html('Favor preencher o campo E-mail corretamente!');
            $(".alert-chat").addClass("alert-danger");
            return false;
        } else {
            email.css('border', '1px solid #818181');
        }
        if (telefone.val() == '') {
            telefone.css('border', '1px solid red');
            telefone.focus();
            $(".alert-chat").html('Favor preencher o campo E-mail!');
            $(".alert-chat").addClass("alert-danger");
            return false;
        } else {
            telefone.css('border', '1px solid #818181');
        }


    $.ajax({
        url: "enviachat.php",
        global: false,
        type: "POST",
        data: ({
            nome: nome.val(),
            telefone: telefone.val(),
            email: email.val(),
            ajax : true
        }),
        dataType: "html",
        success: function(msg){
            var obj = jQuery.parseJSON(msg);
            if (obj.success == true)
            {
                //if ( typeof dataLayer !== "undefined" ) {
                //    dataLayer.push({'event': 'chat/sucesso-botao-de-enviar'});
                //}
                $('.alert-chat').html("Obrigado, agora é só aguardar.");
                $(".alert-chat").addClass("alert-success");
                //aguarda 2 segundos (2000) para fechar a janela
                window.setTimeout(function() { 
                    fecharChatOnline();
                }, 4000);
            }
            else
            {
                $('.alert-chat').html(obj.message);
                $(".alert-chat").addClass("alert-danger");
            }
        }
    });


    var tel = telefone.val().replace(")", "").replace("(", "");
    var arr = tel.split(" ");
    var ddd = arr[0];
    if (arr.length == 2)
        tel = arr[1];
    else if (arr.length == 3)
